{
  "name": "@spec2tools/sdk",
  "version": "0.1.0",
  "description": "Create AI SDK tools from OpenAPI specifications",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "openapi",
    "ai",
    "agent",
    "tools",
    "ai-sdk",
    "vercel-ai"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CahidArda/spec2tools.git",
    "directory": "packages/sdk"
  },
  "dependencies": {
    "@spec2tools/core": "workspace:*",
    "ai": "^6.0.68",
    "zod": "^3.24.0"
  },
  "peerDependencies": {
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
